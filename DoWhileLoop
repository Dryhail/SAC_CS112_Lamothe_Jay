import java.util.Scanner;

//Player plays 3 times.

class RockPaperScissors{

    public static void main(String[] args) {


        Scanner input = new Scanner(System.in);

        int i=1;
        do {
            System.out.println("We are not there yet!" + i);
            i++;}
            while(i<4); 
        }
    }
